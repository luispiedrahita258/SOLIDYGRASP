    protocol Animal {

    func run()
    func speak()
    func swim()
}
lass Salmon: Animal {

    func run() { 
        fatalError("Salmons can NOT run") 
    }

    func speak() { 
        fatalError("Salmons can NOT speak") 
    }

    func swim() { 
        print("Swiming... ğŸŸ  ")
   }
}
